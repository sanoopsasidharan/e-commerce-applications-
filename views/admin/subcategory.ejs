    <script>
        function subDelete(cateId, subcateName) {

            var tr = document.getElementById("tableRowOf" + subcateName);
            var obj = {
                cateId: cateId,
                subcateName: subcateName
            }

            Swal.fire({
                title: 'Are you sure?',
                text: "You won't be able to delete this!",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, delete it!'
            }).then((result) => {
                if (result.isConfirmed) {
                    Swal.fire(
                        'Deleted!',
                        'Your subcategory has been deleted.',
                        'success'
                    ),
                        $.ajax({
                            url: '/admin/deletesubcategory/',
                            method: 'post',
                            data: obj,
                            success(response) {
                                if (response) {
                                    tr.style.display = 'none'
                                } else {
                                }
                            }
                        })

                }
            })


        }
        function addNewSubcategory(d) {
            alert(d)
        }
        $('#addNewSubcategory').submit((e) => {
            e.preventDefault()
            $.ajax({
                url: '/admin/subcategory',
                data: $('#addNewSubcategory').serialize(),
                method: 'post',
                success: (response) => {
                    if (response.status) {
                        location.reload()
                    } else {
                        Swal.fire(
                            'Already exists',
                            'Change the Subcategory name',
                            'error'
                        )
                    }
                }
            })
        })
